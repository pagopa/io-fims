{
  "$id": "https://io.pagopa.it/schemas/fims/access",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "timestamp": {
      "type": "integer"
    },
    "ipAddress": {
      "type": "string",
      "format": "ipv4"
    },
    "fiscalCode": {
      "type": "string"
    },
    "authenticationRequest": {
      "type": "string"
    },
    "idToken": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "timestamp",
    "ipAddress",
    "fiscalCode",
    "authenticationRequest",
    "idToken"
  ]
}
